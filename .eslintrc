{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "indent": "off",
        "object-curly-spacing": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "semi": "off",
        "padded-blocks": "off",
        "quotes": "off",
        "operator-linebreak": "off",
        "max-len": "off",
        "no-case-declarations": "off"
        
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}